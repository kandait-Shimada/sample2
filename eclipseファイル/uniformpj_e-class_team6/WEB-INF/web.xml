<web-app>

	<!-- ListServlet -->
	<servlet>
		<servlet-name>uniformListMapping</servlet-name>
		<servlet-class>servlet.UniformListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>uniformListMapping</servlet-name>
		<url-pattern>/uniformList</url-pattern>
	</servlet-mapping>

	<!-- LoginServlet -->
	<servlet>
		<servlet-name>loginMapping</servlet-name>
		<servlet-class>servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginMapping</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<!-- LogoutServlet -->
	<servlet>
		<servlet-name>logoutMapping</servlet-name>
		<servlet-class>servlet.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logoutMapping</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<!-- InsertIntoCartServlet -->
	<servlet>
		<servlet-name>insertIntoCartMapping</servlet-name>
		<servlet-class>servlet.InsertIntoCartServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insertIntoCartMapping</servlet-name>
		<url-pattern>/insertIntoCart</url-pattern>
	</servlet-mapping>

	<!-- ShowCartServlet -->
	<servlet>
		<servlet-name>showCartMapping</servlet-name>
		<servlet-class>servlet.ShowCartServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>showCartMapping</servlet-name>
		<url-pattern>/showCart</url-pattern>
	</servlet-mapping>

	<!-- BuyConfirmServlet -->
	<servlet>
		<servlet-name>buyConfirmMapping</servlet-name>
		<servlet-class>servlet.BuyConfirmServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>buyConfirmMapping</servlet-name>
		<url-pattern>/buyConfirm</url-pattern>
	</servlet-mapping>

	<!-- ShowOrderedItemServlet -->
	<servlet>
		<servlet-name>showOrderedItemMapping</servlet-name>
		<servlet-class>servlet.ShowOrderedItemServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>showOrderedItemMapping</servlet-name>
		<url-pattern>/showOrderedItem</url-pattern>
	</servlet-mapping>

	<!-- InsertIniDataServlet -->
	<servlet>
		<servlet-name>insertIniDataMapping</servlet-name>
		<servlet-class>servlet.InsertIniDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insertIniDataMapping</servlet-name>
		<url-pattern>/insertIniData</url-pattern>
	</servlet-mapping>


</web-app>